        "react-disqus-comments": "^1.4.0",
        "react-dom": "^16.10.1",
        "react-helmet": "^5.2.1",
        "react-share": "^3.0.1",
        "react-spring": "^8.0.27",
        "react-twitter-widgets": "^1.7.1",
        "redux": "^4.0.5",
        "tailwindcss": "^1.1.4",
